generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id @default(uuid()) // use @ to define primary key
  username   String   @unique
  name       String
  created_at DateTime @default(now())

  @@map("users") // define table name
}
